{
  "expo": {
    "name": "Snek",
    "slug": "snek",
    "description": "Classic Snake Game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/snake-icon.png",
    "splash": {
      "image": "./assets/snake-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.lindapaiste.snek"
    },
    "android": {
      "package": "com.lindapaiste.snek",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/snake-icon.png"
    },
    "githubUrl": "https://github.com/lindapaiste/snake-mobile",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "privacy": "public"
  }
}
